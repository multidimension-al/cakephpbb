{
    "name": "multidimensional/cakephpbb",
    "description": "Add on CakePHP authentication to connect with an external phpBB forum.",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "cakephp3", "authentication"],
    "homepage": "https://github.com/multidimension-al/cakephpbb",
    "license": "MIT",
    "authors": [{
        "name": "multidimension.al",
        "homepage": "https://multidimension.al",
        "role": "Developer"
    }],
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8|^5.6|^6.0",
        "cakephp/cakephp-codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Multidimensional\\Cakephpbb\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Multidimensional\\Cakephpbb\\Test\\": "tests"
        }
    }
}
